{
  "name": "learning-reminder-backend",
  "version": "1.0.0",
  "description": "learning reminder",
  "main": "index.js",
  "repository": "https://github.com/DevRW/learning-reminder-backend.git",
  "author": "itsgracian <gracian2020@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "nodemon --exec  node --require @babel/register --require dotenv/config app.js",
    "start": "node --require @babel/register --require dotenv/config ./app.js",
    "sequelize:init": "yarn global add sequelize-cli",
    "format": "./node_modules/.bin/prettier --write \"**/*.+(js|jsx|json|css|md|ts)\"",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/sequelize db:seed",
    "new:migration": "./node_modules/.bin/sequelize migration:generate --name",
    "new:seed": "./node_modules/.bin/sequelize seed:generate --name",
    "new:model": "./node_modules/.bin/sequelize model:generate --name",
    "migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "africastalking": "^0.4.5-beta.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-code-generator": "^1.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.3",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1",
    "superagent": "^5.2.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5"
  }
}
